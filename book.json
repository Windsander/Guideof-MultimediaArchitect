        "ebook": "styles/common.css",
        "pdf": "styles/common.css",
        "mobi": "styles/common.css",
        "epub": "styles/common.css"
    },
    "plugins": [
        "-lunr", 
        "-search", 
        "search-pro",
		"insert-logo",
		"custom-favicon",
		"tbfed-pagefooter",
		"back-to-top-button",
